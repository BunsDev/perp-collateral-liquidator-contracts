{
  "name": "@perp/perp-collateral-liquidator",
  "version": "1.0.0",
  "description": "Perpetual Protocol Curie (V2) liquidator & contracts",
  "repository": {
    "type": "git",
    "url": "https://github.com/perpetual-protocol/perp-collateral-liquidator.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf typechain && rm -rf artifacts && rm -rf cache",
    "typechain": "npm run typechain-perp-oracle && npm run typechain-perp-curie && npm run typechain-uniswap-v3-core && npm run typechain-uniswap-v3-periphery && npm run typechain-test",
    "typechain-uniswap-v3-core": "typechain --target ethers-v5 --out-dir ./typechain/uniswap-v3-core ./node_modules/@uniswap/v3-core/artifacts/contracts/**/*.json",
    "typechain-uniswap-v3-periphery": "typechain --target ethers-v5 --out-dir ./typechain/uniswap-v3-periphery ./node_modules/@uniswap/v3-periphery/artifacts/contracts/**/*.json",
    "typechain-perp-oracle": "typechain --target ethers-v5 --out-dir ./typechain/perp-oracle ./node_modules/@perp/perp-oracle-contract/artifacts/contracts/**/*.json",
    "typechain-perp-curie": "typechain --target ethers-v5 --out-dir ./typechain/perp-curie ./node_modules/@perp/curie-contract/artifacts/contracts/**/*.json",
    "typechain-test": "typechain --target ethers-v5 --out-dir ./typechain/test ./test/artifacts/**/*.json",
    "build": "npm run typechain && hardhat compile",
    "test": "hardhat test"
  },
  "devDependencies": {
    "@eth-optimism/smock": "1.1.4",
    "@ethereumjs/block": "3.3.0",
    "@ethereumjs/blockchain": "5.3.0",
    "@ethereumjs/common": "2.3.0",
    "@ethereumjs/tx": "3.2.0",
    "@ethereumjs/vm": "5.4.0",
    "@nomiclabs/ethereumjs-vm": "4.2.2",
    "@nomiclabs/hardhat-ethers": "2.0.5",
    "@nomiclabs/hardhat-etherscan": "2.1.7",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@openzeppelin/contracts": "3.4.0",
    "@perp/curie-contract": "1.0.15",
    "@perp/perp-oracle-contract": "0.2.2",
    "@typechain/ethers-v5": "7.0.0",
    "@typechain/hardhat": "2.0.1",
    "@types/chai": "4.2.18",
    "@types/json-stable-stringify": "1.0.33",
    "@types/lodash": "4.14.170",
    "@types/mocha": "9.0.0",
    "@types/node": "15.6.1",
    "@uniswap/v3-core": "1.0.0",
    "@uniswap/v3-periphery": "1.0.1",
    "bignumber.js": "9.0.1",
    "chai": "4.3.4",
    "eslint-config-prettier": "8.3.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.2.0",
    "hardhat": "2.3.0",
    "hardhat-contract-sizer": "2.0.3",
    "hardhat-deploy": "0.9.4",
    "hardhat-deploy-ethers": "0.3.0-beta.11",
    "hardhat-gas-reporter": "1.0.4",
    "husky": "6.0.0",
    "json-stable-stringify": "1.0.1",
    "lint-staged": "11.0.0",
    "lodash": "4.17.21",
    "mocha": "9.1.1",
    "prettier": "2.3.0",
    "prettier-plugin-solidity": "1.0.0-beta.11",
    "shelljs": "0.8.4",
    "solc": "0.7.6",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "solidity-coverage": "0.7.16",
    "ts-generator": "0.1.1",
    "ts-node": "10.0.0",
    "typechain": "5.0.0",
    "typescript": "4.3.2"
  },
  "author": {
    "name": "Perpetual Protocol",
    "email": "hi@perp.fi",
    "url": "https://perp.com/"
  },
  "license": "BSD-3-Clause"
}
